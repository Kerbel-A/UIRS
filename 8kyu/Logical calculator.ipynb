   "source": [
    "def logical_calc(array, op):\n",
    "    out = array[0]\n",
    "    for i in array[1:]:\n",
    "        if op == \"AND\":\n",
    "            out &= i;\n",
    "        elif op == \"OR\":\n",
    "            out |= i;\n",
    "        else:\n",
    "            out ^= i;    \n",
    "    return out"
   ]
  }
